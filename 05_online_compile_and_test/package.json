{
  "name": "05_online_esm",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "lint": "biome check *.ts *.mjs *.js ./src ./test --error-on-warnings",
    "lint:fix": "biome check --apply-unsafe *.ts *.mjs *.js ./src ./test",
    "build": "rollup --config rollup.config.mjs",
    "test:ts-node:cjs": "node --require ts-node/register --enable-source-maps --test test/handler.spec.ts test/exchange-rate-client.spec.ts",
    "test:ts-node:esm": "node --import ./register-ts-node.js --enable-source-maps --test test/handler.spec.ts test/exchange-rate-client.spec.ts",
    "test:ts-node:esm:legacy": "node --experimental-loader ts-node/esm --enable-source-maps --test test/handler.spec.ts test/exchange-rate-client.spec.ts",
    "test:swc:cjs": "node --require @swc-node/register --enable-source-maps --test test/handler.spec.ts test/exchange-rate-client.spec.ts",
    "test:swc:esm": "node --import @swc-node/register/esm-register --enable-source-maps --test ./test/handler.spec.ts ./test/exchange-rate-client.spec.ts",
    "test:tsx:simple": "tsx --enable-source-maps --test test/handler.spec.ts test/exchange-rate-client.spec.ts",
    "test:tsx:loader": "node --import tsx --enable-source-maps --test test/handler.spec.ts test/exchange-rate-client.spec.ts",
    "start": "node dist/main.js"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@swc-node/register": "1.8.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.13.0",
    "msw": "^2.3.1",
    "rollup": "^4.18.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsx": "^4.12.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "express": "^4.19.2",
    "zod": "^3.23.8"
  }
}
