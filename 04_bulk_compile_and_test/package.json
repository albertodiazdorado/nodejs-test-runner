{
	"name": "03_batch_esm",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"lint": "biome check *.ts *.mjs ./src ./test .swcrc package.json --error-on-warnings",
		"lint:fix": "biome check --apply-unsafe *.ts *.mjs ./src ./test .swcrc package.json",
		"compile": "tsc",
		"compile:watch": "tsc --watch",
		"compile:swc": "swc *.ts **/*.ts --out-dir build",
		"build": "rollup --config rollup.config.mjs",
		"test": "node --enable-source-maps --test",
		"test:watch": "node --enable-source-maps --test --watch",
		"start:dev": "node build/main.js",
		"start:prod": "node dist/main.cjs"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.3",
		"@rollup/plugin-commonjs": "^25.0.8",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@swc/cli": "^0.3.12",
		"@swc/core": "^1.5.25",
		"@types/express": "^4.17.21",
		"@types/node": "^20.13.0",
		"msw": "^2.3.1",
		"rollup": "^4.18.0",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"express": "^4.19.2",
		"zod": "^3.23.8"
	}
}
